<div class="col-12-12 main-con">

  <div class="bg-no-status">
    <div class="status-container">
    <!-- Hot Strykes -->
      <div class="col-6-12">
        <div class="stryke-sign">Top Strykes</div>

        <div class="top hide" id="top">
          <%= render partial: @strykes_hot, locals: {top_class: 'top-status'} %>
        </div>
      </div>

    <!-- New Strykes -->

      <div class="col-6-12">
        <div class="stryke-sign">New Strykes</div>
        <div class="new">
          <%= render partial: @strykes_new, locals: {top_class: 'new-status'} %>
        </div>
      </div>
    </div>

    <div class="f-right">
      <img src="assets/up-arrow.png" class='back-to-top'>
    </div>
  </div>

</div>
